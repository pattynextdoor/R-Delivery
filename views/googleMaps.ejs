<!DOCTYPE html>
<html> 
<head>
  <link rel="stylesheet" type="text/css"
    href="sidebar.css"> 
  <meta http-equiv="content-type" content="text/html; charset=UTF-8" /> 
  <title>Google Maps Multiple Markers</title> 
  <script src="http://maps.google.com/maps/api/js?sensor=false" 
          type="text/javascript"></script>
</head> 
<body>
  <!-- Side navigation -->
  <div class="sidenav">
    <a href="#">About</a>
    <a href="#">Services</a>
    <a href="#">Clients</a>
    <a href="#">Contact</a>
  </div>
 
  
    <div id="map" style="width: 500px; height: 400px;"></div>


  <script type="text/javascript">
    var locations = [
      ['Panda Express', 33.97433595651945, -117.32880502939224, 4],
      ['Chronic Tacos', 33.97435597542999, -117.32899412512779, 20],
      ['Scottys Market', 33.97459508983021, -117.32870310544968, 8],
      ['Bookstore', 33.97517785885415, -117.32818007469177, 5],
      ['Coffee Bean', 33.974557276434936, -117.3280593752861, 10]
    ];

    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 16,
      center: new google.maps.LatLng(33.97372315314461, -117.32810497283936),
      mapTypeId: google.maps.MapTypeId.ROADMAP
    });

    var infowindow = new google.maps.InfoWindow();

for (var i = 0; i < locations.length; i++) {
    var marker = new google.maps.Marker({
        position: new google.maps.LatLng(locations[i][1], locations[i][2]),
        map: map,
    });

    var infowindow = new google.maps.InfoWindow({
      content: locations[i][0],
    });
    infowindow.open(map, marker);
}
       </script>
</body>
</html>
